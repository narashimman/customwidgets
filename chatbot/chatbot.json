{
    "id": "com.github.narashimman.cutomwidgets.chatbot",
    "version": "1.0.0",
    "name": "chatbot",
    "description": "sample chat bot",
    "newInstancePrefix": "chatbot",
    "icon": "https://narashimman.github.io/customwidgets/chatbot/icon.png",
    "vendor": "SAP",
    "eula": "",
    "license": "",
    "webcomponents": [
    {
    "kind": "main",
    "tag": "com-github-narashimman-cutomwidgets-chatbot",
    "url": "https://narashimman.github.io/customwidgets/chatbot/chatbot.js",
    "integrity": "",
    "ignoreIntegrity": true
    },
    {
    "kind": "styling",
    "tag": "com-github-narashimman-cutomwidgets-chatbot-styling",
    "url": "https://narashimman.github.io/customwidgets/chatbot/chatbot_styling.js",
    "integrity": "",
    "ignoreIntegrity": true
    },
    {
    "kind": "builder",
    "tag": "com-github-narashimman-cutomwidgets-chatbot-builder",
    "url": "https://narashimman.github.io/customwidgets/chatbot/chatbot_builder.js",
    "integrity": "",
    "ignoreIntegrity": true
    }
    ],
    "properties": {
    "color": {
    "type": "string",
    "description": "Background color",
    "default": "red"
    },
    "opacity": {
    "type": "number",
    "description": "Opacity",
    "default": "1"
    },
    "width": {
    "type": "integer",
    "default": 100
    },
    "height": {
    "type": "integer",
    "default": 100
    }
    },
    "methods": {
    "setColor": {
    "description": "Sets the background color.",
    "parameters": [
    {
    "name": "newColor",
    "type": "string",
    "description": "The new background color"
    }
    ],
    "body": "this.color = newColor;"
    },
    "getColor": {
    "returnType": "string",
    "description": "Returns the background color.",
    "body": "return this.color;"
    }
    },
    "events": {
    "onClick": {
    "description": "Called when the user clicks the Colored Box."
    }
    }
    }
